    "description": "Manager for array Laravel lang files and Bootstrap 4",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.4.*|5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "GuidoCella\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GuidoCella\\LangFileManagerServiceProvider"
            ]
        }
    }
}
